#include <iostream>
using namespace std;
/* Overloading - Same name , diff arguments(diff datatype)*/
void sum(int a,int b);
void sum(float a,float b);
int main()
{

    cout<<"I am inside the main class"<<endl;
    sum(5,6);
    sum(1,2);
    sum(3.5f,2.5f);
	return 0;
}
void sum(int a,int b)
{
    cout<<"Sum of a and b = "<<(a+b)<<endl;
}
void sum(float a,float b)
{
    cout<<"Sum of a, b and c = "<<(a+b)<<endl;
}

